<div class="wa-stack faq-section">
  <h3><%= section %></h3>
  <% faqs.each do |faq| %>
    <wa-details appearance="plain">
      <h4 slot="summary" class="wa-heading-m" style="margin: 0"><%= faq.data.title %></h4>
      <%= markdownify faq.content %>
    </wa-details>
    <% if faq != faqs.last %>
      <wa-divider></wa-divider>
    <% end %>
  <% end %>
</div>






